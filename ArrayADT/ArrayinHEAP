#include<iostream>

using namespace std;

int main(){
    int n{0};
    
    
    int *p;  
    int i;
    p=new int [5];  //dynamic allocation
    p[0]=10;
    p[1]=20;
    p[2]=30;
    p[3]=40;
    p[4]=50;
    
    
    //Array input from user
    
    cout<<"Enter the size of array"<<endl;
    cin>>n;
    int A[n];
    
    cout<<"Enter the array elements: "<<endl;
    
    for(i=0;i<n;i++){
        
        cin>>A[i];
        
    }
    

    
    for(i=0;i<n;i++)
        cout<<A[i]<<' ';
    cout<<endl;
    
    
    //dynamic 
    for(i=0;i<5;i++)
        cout<<p[i]<<' ';
        
    cout<<endl;
    
    return 0;
}
